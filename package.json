{
	"name": "cnode_imitated",
	"version": "1.0.0",
	"description": "a website imitated cnode based on react",
	"main": "index.js",
	"scripts": {
		"build:client": "cross-env NODE_ENV=production webpack --progress --config config/webpack.config.prod.js",
		"build:server": "cross-env NODE_ENV=production webpack --config config/webpack.config.server.js",
		"build:dll": "cross-env NODE_ENV=production webpack --config config/webpack.config.dll.js",
		"dev:client": "cross-env NODE_ENV=development webpack-dev-server --config config/webpack.config.dev.js",
		"dev:server": "cross-env NODE_ENV=development nodemon server/server.js",
		"upload": "node config/upload.js",
		"deploy": "npm run build && npm run upload",
		"build": "npm run build:client && npm run build:server",
		"lint": "eslint --ext .js,.jsx src",
		"precommit": "npm run lint",
		"start": "cross-env NODE_ENV=production node server/server.js",
		"fix": "eslint --fix --ext .js,.jsx src",
		"test:prod": "node config/webpack.config.prod.js"
	},
	"repository": {
		"type": "git",
