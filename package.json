{
  "name": "cnode_imitated",
  "version": "1.0.0",
  "description": "a website imitated cnode based on react",
  "main": "index.js",
  "scripts": {
    "build:client": "cross-env NODE_ENV=development webpack --config config/webpack.config.client.js",
    "build:server": "cross-env NODE_ENV=development webpack --config config/webpack.config.server.js",
    "dev:client": "cross-env NODE_ENV=development webpack-dev-server --config config/webpack.config.client.js",
    "dev:server": "cross-env NODE_ENV=development node server/server.js",
    "build": "npm run build:client && npm run build:server"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/front-end-captain/cnode_imitated.git"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/front-end-captain/cnode_imitated/issues"
  },
  "homepage": "https://github.com/front-end-captain/cnode_imitated#readme",
  "dependencies": {
    "clean-webpack-plugin": "^0.1.18",
    "cross-env": "^5.1.3",
    "express": "^4.16.2",
    "html-webpack-plugin": "^2.30.1",
    "react": "^16.2.0",
    "react-dom": "^16.2.0",
    "react-hot-loader": "^3.1.3",
    "webpack": "^3.10.0"
  },
  "devDependencies": {
    "axios": "^0.17.1",
    "babel-core": "^6.26.0",
    "babel-loader": "^7.1.2",
    "babel-preset-react-app": "^3.1.1",
    "http-proxy-middleware": "^0.17.4",
    "memory-fs": "^0.4.1",
    "webpack-dev-server": "^2.11.1"
  }
}
