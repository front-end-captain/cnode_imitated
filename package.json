  "description": "a website imitated cnode based on react",
  "main": "index.js",
  "scripts": {
    "build:client": "cross-env NODE_ENV=production webpack --config config/webpack.config.prod.js",
    "build:server": "cross-env NODE_ENV=production webpack --config config/webpack.config.server.js",
    "dev:client": "cross-env NODE_ENV=development webpack-dev-server --config config/webpack.config.dev.js",
    "dev:server": "nodemon server/server.js",
    "build": "npm run build:client && npm run build:server",
    "lint": "eslint --ext .js,.jsx src",
    "precommit": "npm run lint",
    "start": "cross-env NODE_ENV=production node server/server.js"
