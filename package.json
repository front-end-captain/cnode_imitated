{
  "name": "cnode_imitated",
  "version": "1.0.0",
  "description": "a website imitated cnode based on react",
  "main": "index.js",
  "scripts": {
    "build:client": "cross-env NODE_ENV=development webpack --config config/webpack.config.client.js",
    "build:server": "cross-env NODE_ENV=development webpack --config config/webpack.config.server.js",
    "dev:client": "cross-env NODE_ENV=development webpack-dev-server --config config/webpack.config.client.js",
    "dev:server": "nodemon server/server.js",
    "build": "npm run build:client && npm run build:server",
    "lint": "eslint --ext .js,.jsx src",
    "precommit": "npm run lint"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/front-end-captain/cnode_imitated.git"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/front-end-captain/cnode_imitated/issues"
  },
  "homepage": "https://github.com/front-end-captain/cnode_imitated#readme",
  "dependencies": {
    "babel-eslint": "^8.2.1",
    "clean-webpack-plugin": "^0.1.18",
    "cross-env": "^5.1.3",
    "express": "^4.16.2",
    "html-webpack-plugin": "^2.30.1",
    "react": "^16.2.0",
    "react-dom": "^16.2.0",
    "react-hot-loader": "^3.1.3",
    "serve-favicon": "^2.4.5",
    "webpack": "^3.10.0"
  },
  "devDependencies": {
    "axios": "^0.17.1",
    "babel-core": "^6.26.0",
    "babel-loader": "^7.1.2",
    "babel-preset-react-app": "^3.1.1",
    "eslint": "^4.17.0",
    "eslint-config-airbnb": "^16.1.0",
    "eslint-config-standard": "^11.0.0-beta.0",
    "eslint-loader": "^1.9.0",
    "eslint-plugin-import": "^2.8.0",
    "eslint-plugin-jsx-a11y": "^6.0.3",
    "eslint-plugin-node": "^5.2.1",
    "eslint-plugin-promise": "^3.6.0",
    "eslint-plugin-react": "^7.6.1",
    "eslint-plugin-standard": "^3.0.1",
    "http-proxy-middleware": "^0.17.4",
    "husky": "^0.14.3",
    "memory-fs": "^0.4.1",
    "webpack-dev-server": "^2.11.1"
  }
}
